{
  "name": "terra",
  "displayName": "terra",
  "description": "Basic terra syntax support",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "terra",
      "aliases": ["Terra", "terra"],
      "extensions": [".t",".terra"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "terra",
      "scopeName": "source.terra",
      "path": "./syntaxes/terra.tmLanguage.json"
    }]
  }
}
