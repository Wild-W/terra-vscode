{
  "name": "terra-lua",
  "displayName": "Terra-Lua",
  "description": "Basic Terra syntax support",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.93.0"
  },
  "publisher": "WildW",
  "categories": [
    "Programming Languages"
  ],
  "icon": "./icon.png",
  "author": {
    "email": "connor.bren@gmail.com",
    "name": "Connor Bren",
    "url": "https://github.com/Wild-W"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Wild-W/terra-vscode"
  },
  "bugs": {
    "url": "https://github.com/Wild-W/terra-vscode/issues"
  },
  "contributes": {
    "languages": [{
      "id": "terra",
      "aliases": ["Terra", "terra"],
      "extensions": [".t",".terra"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "terra",
      "scopeName": "source.terra",
      "path": "./syntaxes/terra.tmLanguage.json"
    }]
  }
}
